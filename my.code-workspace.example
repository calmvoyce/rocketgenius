{
	"folders": [
		{
			"path": "."
		}
	],
	"settings": {
		"runTerminalCommand.commands": [
			{
				"command": "bash /workspace/workspace.sh init",
				"name": "Init all Projects",
				"group": "Workspace's Command Line",
				"auto": true
			},
			{
				"command": "bash /workspace/workspace.sh setup $PWD",
				"name": "Setup Current Project",
				"group": "Workspace's Command Line",
				"auto": true
			},
			{
				"command": "composer update",
				"name": "Update Composer",
				"group": "Composer's Commands",
				"auto": true
			}
			{
				"command": "composer install",
				"name": "Install Composer",
				"group": "Composer's Commands",
				"auto": true
			}
		],
		"intelephense.format.enable": false,
		"[php]": {
			"editor.defaultFormatter": "valeryanm.vscode-phpsab"
		},
		"git.ignoreLimitWarning": true,
		"editor.formatOnSave": true,
		"editor.insertSpaces": false,
		"editor.detectIndentation": false,
		"editor.formatOnPaste": true,
		"editor.formatOnSaveMode": "modifications",
		"phpsab.autoRulesetSearch": false,
		"phpsab.debug": true,
		"phpsab.executablePathCBF": "/workspace/vendor/bin/phpcbf",
		"phpsab.executablePathCS": "/workspace/vendor/bin/phpcs",
		"phpsab.fixerEnable": true,
		"phpsab.snifferShowSources": true,
		"phpsab.snifferMode": "onSave",
		"phpsab.standard": "Inpsyde",
		"phpunit.files": "{test,tests,Test,Tests}/**/*Test.php"
	}
}