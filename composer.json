{
  "name": "mariacdadalt/workspace",
  "description": "Maria C. Dadalt's Workspace",
  "type": "project",
  "license": "GPLv2",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "7.4.7"
    },
    "sort-packages": true,
    "preferred-install": {
      "*": "dist",
      "mariacdadalt": "source"
	},
	"allow-plugins": {
		"dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
	"inpsyde/php-coding-standards": "^1.0",
	"squizlabs/php_codesniffer": "^3.6",
	"phpunit/phpunit": "^9.5"
  }
}
